<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home"
    app:startDestination="@+id/tabMenuFragment">

    <fragment
        android:id="@+id/tabMenuFragment"
        android:name="com.onix.internship.ui.tabMenu.TabMenuFragment"
        android:label="TabMenuFragment">

        <action
            android:id="@+id/action_tabMenuFragment_to_addCityFragment"
            app:destination="@id/addCityFragment" />

        <action
            android:id="@+id/action_tabMenuFragment_to_cityInfoFragment"
            app:destination="@id/cityInfoFragment" />
    </fragment>

    <fragment
        android:id="@+id/addCityFragment"
        android:name="com.onix.internship.ui.addCity.AddCityFragment"
        android:label="AddCityFragment" />

    <fragment
        android:id="@+id/cityInfoFragment"
        android:name="com.onix.internship.ui.cityMoreInfo.CityInfoFragment"
        android:label="CityInfoFragment" />

</navigation>